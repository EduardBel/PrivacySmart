{

    "manifest_version": 2,
    "name": "Plugin TFG",
    "version": "1.0",
    
    "description": "Canviant manera de detectar vendor.",
    "icons": {
      "48": "icons/border-48.png"
    },
    "browser_action": {
      "default_icon": "icons/border-48.png",
      "default_title": "TFG Eduard",
      "default_popup": "popup/index.html",
      "browser_style": true
    },  
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "activeTab",
        "tabs",
        "webNavigation",
        "<all_urls>"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["web3.min.js","scripts.js","vendor_scripts/cookieyes.js","vendor_scripts/cookiebot.js","vendor_scripts/didomi.js","content.js"]
      }
    ]
    
  
  }